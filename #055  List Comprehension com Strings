#Utilizado quando precisa criar uma lista a partir de uma lista existente
# Expressão = [expressão for iten in items]
frutas1 = ['abacate', 'banana', 'morango', 'kiwi', 'abacaxi']

#frutas2 = []

#for iten in frutas1:
#    if 'b' in iten:
#      frutas2.append(iten)

frutas2 = [iten for iten  in frutas1 if 'b' in iten]

print(frutas2)
